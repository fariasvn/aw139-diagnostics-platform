# AW139 Smart Troubleshooting Assistant - Environment Variables
# Copy this file to .env and fill in the values

# === REQUIRED ===

# OpenAI API key for embeddings and GPT-4
OPENAI_API_KEY=

# PostgreSQL database connection string (Neon serverless)
DATABASE_URL=

# Session secret for Express session management
SESSION_SECRET=

# === OPTIONAL ===

# OpenAI embedding model (default: text-embedding-3-small)
EMBEDDING_MODEL=text-embedding-3-small

# Server ports (defaults shown)
PORT=5000
RAG_API_PORT=8000
CREW_API_PORT=9000

# Internal service URLs (usually don't need to change)
RAG_API_URL=http://127.0.0.1:8000
CREWAI_URL=http://127.0.0.1:9000

# Dropbox integration (optional, for data sync)
# Configured via Replit Secrets when using Replit

# IETP Connector (optional, for local IETP integration)
IETP_CONNECTOR_URL=
IETP_CONNECTOR_TOKEN=
